{
  "manifestVersion": 1,
  "meta": {
    "name": "Dogenet",
    "version": "0.0.1",
    "logoPath": "logo.png",
    "shortDescription": "Node to Node communication protocol",
    "longDescription": "",
    "upstreamVersions": null
  },
  "config": {
    "sections": null
  },
  "container": {
    "build": {
      "nixFile": "pup.nix",
      "nixFileSha256": "b6c62d0f7221b9f136a444d31b3c1f01e584598f41c9ef433727c968d24f3f6b"
    },
    "services": [
      {
        "name": "dogenet",
        "command": {
          "exec": "/bin/run.sh",
          "cwd": "",
          "env": null
        }
      }
    ],
    "exposes": [
      {
        "name": "dogenet-comms",
        "type": "tcp",
        "port": 42069,
        "listenOnHost": true
      },
      {
        "name": "handler",
        "type": "tcp",
        "port": 42068,
        "interfaces": ["dogenet-handler"]
      },
      {
        "name": "web-api",
        "type": "http",
        "port": 8080,
        "interfaces": ["dogenet-web-api-nodes"]
      }
    ],
    "requiresInternet": false
  },
  "interfaces": [
    {
      "name": "dogenet-handler",
      "version": "0.0.1",
      "permissionGroups": [
        {
          "name": "Handler",
          "description": "Allows a new Dogenet Handler to be registered",
          "severity": 2,
          "port": 42068
        }
      ]
    },
    {
      "name": "dogenet-web-api-nodes",
      "version": "0.0.1",
      "permissionGroups": [
        {
          "name": "Read API Nodes",
          "description": "Allows read-only access to Node data",
          "severity": 1,
          "routes": ["/nodes"]
        }
      ]
    }
  ],
  "dependencies": [],
  "metrics": null
}
